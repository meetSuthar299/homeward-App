{
  "main": "src/Main.js",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest --passWithNoTests",
    "lint": "eslint . --ext .js,.jsx"
  },
  "dependencies": {
    "@react-native-community/masked-view": "0.1.10",
    "@react-native-community/segmented-control": "2.1.1",
    "@react-navigation/native": "^5.8.10",
    "@react-navigation/stack": "^5.12.8",
    "expo": "~39.0.2",
    "expo-constants": "~9.2.0",
    "expo-facebook": "~9.0.0",
    "expo-google-app-auth": "^8.1.3",
    "expo-image-picker": "~9.1.1",
    "expo-location": "~9.0.0",
    "expo-status-bar": "~1.0.2",
    "firebase": "7.9.0",
    "formik": "^2.2.5",
    "lottie-react-native": "~2.6.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-geocode": "^0.2.2",
    "react-native": "https://github.com/expo/react-native/archive/sdk-39.0.3.tar.gz",
    "react-native-dropdown-picker": "^3.7.1",
    "react-native-gesture-handler": "~1.7.0",
    "react-native-google-places-autocomplete": "^2.1.1",
    "react-native-keyboard-aware-scroll-view": "^0.9.3",
    "react-native-maps": "0.27.1",
    "react-native-modal": "^11.5.6",
    "react-native-picker-select": "^8.0.3",
    "react-native-reanimated": "~1.13.0",
    "react-native-safe-area-context": "3.1.4",
    "react-native-screens": "~2.10.1",
    "react-native-svg": "12.1.0",
    "react-native-web": "~0.13.7",
    "rn-placeholder": "^3.0.3",
    "yup": "^0.31.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/eslint-parser": "^7.11.5",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "eslint": "^7.12.1",
    "eslint-config-universe": "^5.0.0",
    "eslint-plugin-jest": "^24.1.0",
    "jest-expo": "^39.0.0",
    "prettier": "^2.1.2",
    "react-test-renderer": "^16.13.1",
    "typescript": "^4.1.2"
  },
  "eslintConfig": {
    "root": true,
    "parser": "@babel/eslint-parser",
    "plugins": [
      "jest"
    ],
    "extends": [
      "universe/native",
      "plugin:jest/recommended",
      "plugin:jest/style"
    ],
    "rules": {
      "prettier/prettier": "error",
      "radix": [
        "error",
        "as-needed"
      ],
      "linebreak-style": [
        "warn",
        "unix"
      ]
    }
  },
  "prettier": {
    "endOfLine": "lf",
    "singleQuote": true,
    "arrowParens": "avoid",
    "jsxSingleQuote": true,
    "trailingComma": "none",
    "bracketSpacing": true
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*)"
    ]
  },
  "resolutions": {
    "jpeg-js": "^0.4.0"
  }
}
